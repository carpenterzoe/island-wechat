
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.header{
  position: fixed;
  background-color: #fff;
  height: 100rpx;
  width: 100%;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 0 3px #e3e3e3;
  z-index: 99;
}

.head-img {
  width: 106rpx;
  height: 34rpx;
  margin-top: 40rpx;
}

.box{
  border-radius: 50px;
  background-color: #f5f5f5;
  display: flex;
  width:700rpx;
  height: 68rpx;
  align-items: center;
  justify-content: center;
  color:#999999;
  font-size: 30rpx;
}

.box image{
  width: 14px;
  height: 14px;
  margin-right: 10px;
  margin-bottom: -2px;
}

.sub-container{
  margin-top: 100rpx;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.head-img{
  width: 106rpx;
  height: 34rpx;
  margin-top: 40rpx;
}

.book-container{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  padding: 0 90rpx;
  margin-top: 20rpx;
}

.book-container v-book {
  margin-bottom: 30rpx;
}